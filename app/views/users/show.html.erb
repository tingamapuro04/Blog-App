<div>
  <%= render 'cross' %>
  <div>
    <h4>Bio</h4>
    <p><%= @user2.bio %></p>
  </div>
  <% @user2.recent_posts.each do |post| %>
    <div>
      <p>Post <%= post.id %></p>
      <p><%= post.text %></p>
      <p>Comments: <%= post.count_comments %></p>
      <p>Likes: <%= post.count_likes %></p>
    </div>

  <% end %>
</div>

<button>See all posts</button>
